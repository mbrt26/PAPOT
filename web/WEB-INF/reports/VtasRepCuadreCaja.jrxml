	<queryString>
		<![CDATA[SELECT           tblPagos.idLocal,
                 COUNT(*) AS idRecibo,
                 '0' AS idCliente,
                 'VENTAS CONTADO/ABONOS' AS nombreTercero,
                 SUM(tblPagos.vrPago) AS vrBase,
                 'VENTAS' AS nombreTipoOrden,
                 1 AS idAlcance,
                 SUM(tblPagos.vrPago) AS vrDcto,
                 $P{p_saldoInicial}  AS saldoInicial,
                 SUM(tblPagos.vrPago) + $P{p_saldoInicial}
                                     AS saldoFinal,
                'INGRESO' AS tipoOrden
 FROM   tblPagos
 WHERE EXISTS (
    SELECT tblPagosMedios.*
    FROM tblPagosMedios
    WHERE tblPagos.idLocal              =
                     tblPagosMedios.idLocal
    AND tblPagos.idTipoOrden            =
                 tblPagosMedios.idTipoOrden
    AND tblPagos.idRecibo               =
                    tblPagosMedios.idRecibo
    AND tblPagos.idLog                  =
                       tblPagosMedios.idLog
    AND tblPagos.indicador              =
                   tblPagosMedios.indicador)

AND   tblPagos.idLocal             =
                         $P{p_idLocal}
AND tblPagos.idTipoOrden IN (9,29)
AND tblPagos.indicador BETWEEN $P{p_indicadorINI}
                            AND $P{p_indicadorFIN}
                AND tblPagos.fechaPago
                 BETWEEN $P{p_fechaInicialSqlServer}
                 AND $P{p_fechaFinalSqlServer}
GROUP BY tblPagos.idLocal
UNION
SELECT           tblPagos.idLocal,
                 tblPagos.idRecibo,
                 tblPagos.nitCC AS idCliente,
                 ( SELECT TOP 1 tblTerceros.nombreTercero
                   FROM tblTerceros
                   WHERE tblTerceros.idCliente =
                    tblPagos.nitCC ) AS nombreTercero,
                 tblPagos.vrPago AS vrBase,
                 'PAGO PROVEEDOR' AS nombreTipoOrden,
                 5 AS idAlcance,
                 tblPagos.vrPago * (-1) AS vrDcto,
                 0 AS saldoInicial,
                 tblPagos.vrPago * (-1) AS saldoFinal,
                'EGRESO' AS tipoOrden
 FROM   tblPagos
 WHERE EXISTS (
    SELECT tblPagosMedios.*
    FROM tblPagosMedios
    WHERE tblPagos.idLocal              =
                     tblPagosMedios.idLocal
    AND tblPagos.idTipoOrden            =
                 tblPagosMedios.idTipoOrden
    AND tblPagos.idRecibo               =
                    tblPagosMedios.idRecibo
    AND tblPagos.idLog                  =
                       tblPagosMedios.idLog
    AND tblPagos.indicador              =
                   tblPagosMedios.indicador)

AND   tblPagos.idLocal             =
                         $P{p_idLocal}
AND tblPagos.idTipoOrden IN (1,21)
AND tblPagos.indicador BETWEEN $P{p_indicadorINI}
                            AND $P{p_indicadorFIN}
                AND tblPagos.fechaPago
                 BETWEEN $P{p_fechaInicialSqlServer}
                 AND $P{p_fechaFinalSqlServer}
UNION
SELECT     tblDctos.idLocal,
           tblDctos.idDcto AS idRecibo,
           tblDctos.idCliente,
           tblDctos.nombreTercero,
           tblDctos.vrBase,
           tblTipoOrden.nombreTipoOrden,
           tblTipoOrden.idAlcance,
          (CASE WHEN
                 tblTipoOrden.idAlcance < 5
           THEN  tblDctos.vrBase
           ELSE  tblDctos.vrBase * (-1)
         END) AS vrDcto,
          0 AS saldoInicial,
          (CASE WHEN
                 tblTipoOrden.idAlcance < 5
           THEN  tblDctos.vrBase
           ELSE  tblDctos.vrBase * (-1)
         END) AS saldoFinal,
          (CASE WHEN
                 tblTipoOrden.idAlcance < 5
           THEN  'INGRESO'
           ELSE  'EGRESO'
         END) AS tipoOrden
FROM       tblDctos
INNER JOIN tblTipoOrden
ON tblDctos.IDTIPOORDEN                    =
                      tblTipoOrden.idTipoOrden
WHERE     EXISTS
 (SELECT  *
  FROM    tblDctosOrdenesDetalle
  WHERE tblDctosOrdenesDetalle.idLocal     =
                              tblDctos.IDLOCAL
  AND   tblDctosOrdenesDetalle.idTipoOrden =
                          tblDctos.idTipoOrden
  AND   tblDctosOrdenesDetalle.idOrden     =
                             tblDctos.idOrden)
AND tblTipoOrden.idAlcance             IN (4,5)
AND   tblDctos.IDLOCAL             =
                         $P{p_idLocal}
AND tblDctos.indicador BETWEEN $P{p_indicadorINI}
                            AND $P{p_indicadorFIN}
                AND tblDctos.fechaDcto
                 BETWEEN $P{p_fechaInicialSqlServer}
                 AND $P{p_fechaFinalSqlServer}
ORDER BY 1,7]]>
