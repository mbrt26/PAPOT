	<queryString>
		<![CDATA[SELECT tblLineas.nombreLinea,
       tblCategorias.idLinea,
       tblCategorias.IdCategoria,
       tblCategorias.nombreCategoria,
       tblMarcas.idMarca,
       tblMarcas.nombreMarca,
       tblPlus.idPlu,
       tblPlus.nombrePlu,
       $P{p_idLocal} AS idLocal
FROM   tblCategorias
INNER JOIN tblLineas
ON tblCategorias.idLinea      =
                   tblLineas.idLinea
INNER JOIN tblPlus
ON tblCategorias.idLinea      =
                     tblPlus.idLinea
AND tblCategorias.IdCategoria =
                 tblPlus.idCategoria
INNER JOIN tblMarcas
ON tblPlus.idMarca =
                  tblMarcas.idMarca
WHERE tblPlus.idPlu = $P{p_idPlu}]]>
