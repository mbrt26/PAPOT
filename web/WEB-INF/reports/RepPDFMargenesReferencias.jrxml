	<queryString>
		<![CDATA[begin tran
if ($P{p_sk_tercero}=0)
begin
SELECT
 [dbo].[tblCotClienteRef].[idCliente],
 [nombreCliente],
 [dbo].[tblCotClienteRef].idPlu,
 [dbo].[tblCotClienteRef].ficha,
 [dbo].[tblCotClienteRef].ancho,
 [dbo].[tblCotClienteRef].largo,
 [dbo].[tblCotClienteRef].calibre,
 [dbo].[tblCotClienteRef].referencia,
 [nombreReferencia], 
 precioUnitario as PrecioVenta,
 ROUND( 100*((bache*1000/pesoMillar*precioUnitario)-(CostosProcesos+CostosMateria+flete*bache))/(bache*1000/pesoMillar*precioUnitario),2) AS MARGEN  
 FROM [BDCommerce].[dbo].[tblCotClienteRef] 	
 INNER JOIN [dbo].[vistaCostoProceso] ON [dbo].[tblCotClienteRef].idPlu=[dbo].[vistaCostoProceso].idplu
 INNER JOIN [dbo].[vistaCostoMateria] ON [dbo].[tblCotClienteRef].idPlu=[dbo].[vistaCostoMateria].idplu
 INNER JOIN [dbo].[tblClienteCotizacion] ON [dbo].[tblClienteCotizacion].idCliente=[dbo].[tblCotClienteRef].idCliente
 WHERE  [dbo].[tblCotClienteRef].idCliente <> 0
 ORDER BY [dbo].[tblClienteCotizacion].nombreCliente,MARGEN;
end 
else
begin
SELECT
 [dbo].[tblCotClienteRef].[idCliente],
 [nombreCliente],
 [dbo].[tblCotClienteRef].idPlu,
 [dbo].[tblCotClienteRef].ficha,
 [dbo].[tblCotClienteRef].ancho,
 [dbo].[tblCotClienteRef].largo,
 [dbo].[tblCotClienteRef].calibre,
 [dbo].[tblCotClienteRef].referencia,
 [nombreReferencia], 
 precioUnitario as PrecioVenta,
 ROUND( 100*((bache*1000/pesoMillar*precioUnitario)-(CostosProcesos+CostosMateria+flete*bache))/(bache*1000/pesoMillar*precioUnitario),2) AS MARGEN  
 FROM [BDCommerce].[dbo].[tblCotClienteRef] 	
 INNER JOIN [dbo].[vistaCostoProceso] ON [dbo].[tblCotClienteRef].idPlu=[dbo].[vistaCostoProceso].idplu
 INNER JOIN [dbo].[vistaCostoMateria] ON [dbo].[tblCotClienteRef].idPlu=[dbo].[vistaCostoMateria].idplu
 INNER JOIN [dbo].[tblClienteCotizacion] ON [dbo].[tblClienteCotizacion].idCliente=[dbo].[tblCotClienteRef].idCliente
 WHERE  [dbo].[tblCotClienteRef].idCliente = $P{p_sk_tercero}
 ORDER BY [dbo].[tblClienteCotizacion].nombreCliente,MARGEN;
end    
commit tran]]>
