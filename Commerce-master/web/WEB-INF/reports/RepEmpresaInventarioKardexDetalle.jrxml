      (SELECT ISNULL(SUM(tmpSDO.cantidadSalida),0.0)  +
              ISNULL(SUM(tmpSDO.cantidadEntrada),0.0) -
             ( SELECT tblPlusInventario.existencia
        FROM tblPlusInventario
