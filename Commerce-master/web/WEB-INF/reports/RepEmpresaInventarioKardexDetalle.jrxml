WHERE tblDctos.fechaDcto BETWEEN $P{p_fechaInicial}
                             AND $P{p_fechaFinal}
AND tblDctosOrdenesDetalle.idPlu = $P{p_idPlu}
AND tblDctosOrdenesDetalle.idBodega = $P{p_idBodega})
                               AS tmpSDO )
                             AS cantidad,
       0.0 AS cantidadEntrada,
       0.0 AS cantidadSalida,
       '  ' AS nombreCategoria,
       '  ' AS nombreMarca,
       '  ' AS nombreLinea,
       tblPlus.idPlu,
       tblPlus.nombrePlu,
       0.0 AS saldoInicial
FROM tblPlus
WHERE tblPlus.idPlu = $P{p_idPlu}
UNION
SELECT tblTipoOrden.nombreTipoOrden,
       ctrlUsuarios.aliasUsuario,
       tmpDET.IDLOCAL,
       tmpDET.idDcto,
       tmpDET.idCliente,
